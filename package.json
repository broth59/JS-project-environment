  "scripts": {
    "build:all": "tsc --build ./tsconfig.json",
    "build:server": "tsc --build ./src/server/tsconfig.json",
    "test:server": "jest --config=config/jest/jest.config.server.js --watch",
    "test-update:server": "jest --config=config/jest/jest.config.server.js",
    "build:client": "export NODE_ENV=PRODUCTION  && export TS_NODE_PROJECT=./src/client/tsconfig.json && ts-node -r tsconfig-paths/register ./scripts/build.ts ",
    "watch:client": "export NODE_ENV=DEVELOPMENT && export TS_NODE_PROJECT=./src/client/tsconfig.json && ts-node -r tsconfig-paths/register ./scripts/watch.ts",
    "test:client": "jest --config=config/jest/jest.config.client.js --watch",
    "test-update:client": "jest --config=config/jest/jest.config.client.js",
    "clear": "rm -rf dist && rm -rf coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
